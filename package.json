{
  "name": "sprout",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean && npm i",
    "test": "ava --verbose",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s"
  },
  "dependencies": {
    "analytics-react-native": "^1.2.0",
    "apisauce": "0.11.0",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "aws-sdk": "^2.82.0",
    "buffer": "^5.2.1",
    "d3-scale": "^1.0.6",
    "d3-shape": "^1.2.2",
    "fuse.js": "^3.4.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jwt-decode": "^2.2.0",
    "lodash": "4.17.19",
    "lottie-react-native": "^2.5.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.23",
    "react": "^16.3.1",
    "react-native": "^0.55.3",
    "react-native-animatable": "^1.3.0",
    "react-native-aws-cognito-js": "0.0.5",
    "react-native-branch": "^3.0.0-beta.3",
    "react-native-cached-image": "^1.4.3",
    "react-native-circular-progress": "^0.2.0",
    "react-native-code-push": "^5.2.1",
    "react-native-communications": "^2.2.1",
    "react-native-contacts-wrapper": "^0.2.4",
    "react-native-elements": "^0.19.1",
    "react-native-fbsdk": "^0.8.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fs": "^2.11.17",
    "react-native-geocoder": "^0.5.0",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-gesture-responder": "^0.1.1",
    "react-native-gifted-chat": "^0.4.3",
    "react-native-google-analytics-bridge": "^6.1.0",
    "react-native-highcharts": "^0.2.1",
    "react-native-image-picker": "^0.26.10",
    "react-native-image-slider": "^2.0.3",
    "react-native-keyboard-aware-scroll-view": "^0.5.0",
    "react-native-linear-gradient": "^2.4.2",
    "react-native-lottie-loader": "0.0.5",
    "react-native-maps": "git+https://github.com/AnitaSinghLoved/react-native-maps.git",
    "react-native-mixpanel": "^1.0.2",
    "react-native-modal": "^6.5.0",
    "react-native-navigation": "^1.1.479",
    "react-native-pdf": "^2.0.6",
    "react-native-picker": "^4.3.7",
    "react-native-push-notification": "^3.0.2",
    "react-native-reanimated": "^1.0.0-alpha.12",
    "react-native-render-html": "^3.10.0",
    "react-native-search-bar": "^3.4.2",
    "react-native-sentry": "^0.38.3",
    "react-native-share": "^1.1.3",
    "react-native-sms": "^1.8.0",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-spinkit": "^1.1.1",
    "react-native-svg": "^8.0.8",
    "react-native-svg-charts": "^5.2.0",
    "react-native-swiper": "^1.5.13",
    "react-native-tab-navigator": "^0.3.4",
    "react-native-tab-view": "^2.0.1",
    "react-native-text-input-mask": "^0.7.0",
    "react-native-touch-id": "^4.0.5",
    "react-native-typewriter": "^0.6.1",
    "react-native-uuid-generator": "^5.0.0",
    "react-native-vector-icons": "^5.0.0",
    "react-redux": "5.0.4",
    "react-swipeable-views-native": "^0.12.8",
    "recyclerlistview": "^1.3.4",
    "redux": "3.6.0",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.14.6",
    "reduxsauce": "0.4.1",
    "seamless-immutable": "7.1.2",
    "validate.js": "^0.11.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-preset-es2015": "^6.18.0",
    "code-push-cli": "^2.1.6",
    "enzyme": "^2.6.0",
    "husky": "^0.13.1",
    "ignite-ir-boilerplate-2016": "^0.2.2",
    "ignite-vector-icons": "^0.2.1",
    "jasmine": "^2.6.0",
    "jest": "^19.0.2",
    "mockery": "^2.0.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.4.0",
    "react-native-mock": "^0.3.1",
    "reactotron-apisauce": "^1.7.0",
    "reactotron-react-native": "^1.7.0",
    "reactotron-redux": "^1.7.0",
    "reactotron-redux-saga": "^1.7.0",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0"
  },
  "ava": {
    "files": [
      "Tests/**/*.js",
      "!Tests/Setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./Tests/Setup"
    ],
    "babel": "inherit"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "if [ -d 'ignite-base' ]; then cd ignite-base; fi; npm run lint"
    }
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
